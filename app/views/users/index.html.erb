<p id="notice"><%= notice %></p>

<h1 class="title is-1">Listing Users</h1>

<table>
  <thead>
    <tr>
      <h2 class="subtitle"><th>Name</th></h2>
      <h2 class="subtitle"><th>Email</th></h2>
      <th colspan="3"></th>
    </tr>
  </thead>

<tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
       
      </tr>
      <td>
        <p><%= link_to 'Show', user %></p>      
      
      </td>

    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_registration_path %>
